{
  "name": "dslink-dart-schedule",
  "version": "0.0.5",
  "description": "Schedule DSLink",
  "main": "bin/run.dart",
  "engines": {
    "dart": ">=1.13.0"
  },
  "getDependencies": [
    "pub get"
  ],
  "configs": {
    "broker": {
      "type": "url"
    },
    "name": {
      "type": "string",
      "default": "Schedule"
    },
    "token": {
      "type": "string"
    },
    "log": {
      "type": "enum[info,off,fine,finest,finer]",
      "default": "info"
    }
  }
}
